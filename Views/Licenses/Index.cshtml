@model IEnumerable<ZuHuanJingDemo2.Models.License>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.License_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.License_Introduction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.License_CreateDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model is not null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.License_Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.License_Introduction)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.License_CreateDate)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.License_Id">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.License_Id">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.License_Id">Delete</a>
                    </td>
                </tr>
            }   }
    </tbody>
</table>
